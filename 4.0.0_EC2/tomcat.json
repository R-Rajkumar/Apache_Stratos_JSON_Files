{  
   "type":"tomcat",
   "provider":"apache",
   "host":"stratos.org",
   "displayName":"Apache Tomcat",
   "description":"Apache Tomcat Cartridge",
   "version":"7.0",
   "multiTenant":"false",
   "portMapping":[  
      {  
         "protocol":"http",
         "port":"80",
         "proxyPort":"80"
      },
      {  
         "protocol":"https",
         "port":"443",
         "proxyPort":"443"
      }
   ],
   "deployment":{  

   },
   "iaasProvider":[  
      {  
         "type":"ec2",
         "imageId":"ap-southeast-1/ami-3c0f5d6e",
         "maxInstanceLimit":"4",
         "property":[  
            {  
               "name":"instanceType",
               "value":"m1.medium"
            }
         ]
      }
   ],
   "loadBalancer":{  
      "type":"lb",
      "property":{  
         "name":"default.load.balancer",
         "value":"true"
      }
   }
}
